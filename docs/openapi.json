{
  "openapi": "3.0.3",
  "info": {
    "title": "Alleato Procore API",
    "description": "Comprehensive API documentation for the Alleato Procore application.\n\nThis API provides endpoints for:\n- Project management\n- Financial management (commitments, contracts, invoices)\n- RAG-based chat functionality\n- Authentication and authorization\n- Company management\n- Document search and insights\n\nThe API is split between:\n- **Backend API** (FastAPI at port 8051) - Handles RAG chat and agent workflows\n- **Frontend API** (Next.js at port 3000/3001) - Handles CRUD operations and Supabase integration\n",
    "version": "1.0.0",
    "contact": {
      "name": "Alleato Support",
      "email": "support@alleato.com"
    },
    "license": {
      "name": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  },
  "servers": [
    {
      "url": "http://localhost:3000/api",
      "description": "Next.js Frontend API (Development)"
    },
    {
      "url": "http://localhost:3001/api",
      "description": "Next.js Frontend API (Alternative Port)"
    },
    {
      "url": "http://localhost:8051",
      "description": "FastAPI Backend (RAG/Chat Services)"
    }
  ],
  "tags": [
    {
      "name": "Projects",
      "description": "Project management operations"
    },
    {
      "name": "Commitments",
      "description": "Financial commitment management"
    },
    {
      "name": "Companies",
      "description": "Company directory operations"
    },
    {
      "name": "Authentication",
      "description": "User authentication and authorization"
    },
    {
      "name": "RAG Chat",
      "description": "RAG-based chat and agent operations"
    },
    {
      "name": "Health",
      "description": "System health checks"
    },
    {
      "name": "AI",
      "description": "AI-powered features"
    },
    {
      "name": "Search",
      "description": "Document and content search"
    }
  ],
  "paths": {
    "/health": {
      "get": {
        "tags": [
          "Health"
        ],
        "summary": "Backend health check",
        "description": "Verifies backend status and OpenAI configuration",
        "servers": [
          {
            "url": "http://localhost:8051"
          }
        ],
        "responses": {
          "200": {
            "description": "Backend is healthy",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "healthy"
                    },
                    "openai_configured": {
                      "type": "boolean"
                    },
                    "rag_available": {
                      "type": "boolean"
                    },
                    "timestamp": {
                      "type": "string",
                      "format": "date-time"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/health": {
      "get": {
        "tags": [
          "Health"
        ],
        "summary": "Frontend API health check",
        "description": "Checks the health of the frontend API and backend connection",
        "responses": {
          "200": {
            "description": "API is healthy",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "ok"
                    },
                    "backend": {
                      "type": "object",
                      "properties": {
                        "connected": {
                          "type": "boolean"
                        },
                        "url": {
                          "type": "string"
                        },
                        "openai_configured": {
                          "type": "boolean"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/projects": {
      "get": {
        "tags": [
          "Projects"
        ],
        "summary": "List all projects",
        "description": "Retrieve a paginated list of projects with optional filters",
        "parameters": [
          {
            "in": "query",
            "name": "page",
            "schema": {
              "type": "integer",
              "default": 1
            },
            "description": "Page number"
          },
          {
            "in": "query",
            "name": "limit",
            "schema": {
              "type": "integer",
              "default": 100
            },
            "description": "Items per page"
          },
          {
            "in": "query",
            "name": "search",
            "schema": {
              "type": "string"
            },
            "description": "Search by project name or job number"
          },
          {
            "in": "query",
            "name": "state",
            "schema": {
              "type": "string"
            },
            "description": "Filter by project state (case-insensitive)"
          },
          {
            "in": "query",
            "name": "excludeState",
            "schema": {
              "type": "string"
            },
            "description": "Exclude projects with specific state"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/Project"
                      }
                    },
                    "meta": {
                      "$ref": "#/components/schemas/PaginationMeta"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/ServerError"
          }
        }
      },
      "post": {
        "tags": [
          "Projects"
        ],
        "summary": "Create a new project",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateProject"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Project created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Project"
                }
              }
            }
          },
          "500": {
            "$ref": "#/components/responses/ServerError"
          }
        }
      }
    },
    "/api/projects/{id}": {
      "get": {
        "tags": [
          "Projects"
        ],
        "summary": "Get project details",
        "servers": [
          {
            "url": "http://localhost:8051"
          }
        ],
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "integer"
            },
            "description": "Project ID"
          }
        ],
        "responses": {
          "200": {
            "description": "Project details with tasks and insights",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "project": {
                      "$ref": "#/components/schemas/Project"
                    },
                    "tasks": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/Task"
                      }
                    },
                    "insights": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/Insight"
                      }
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Project not found"
          }
        }
      }
    },
    "/api/commitments": {
      "get": {
        "tags": [
          "Commitments"
        ],
        "summary": "List commitments",
        "parameters": [
          {
            "in": "query",
            "name": "page",
            "schema": {
              "type": "integer",
              "default": 1
            }
          },
          {
            "in": "query",
            "name": "limit",
            "schema": {
              "type": "integer",
              "default": 10
            }
          },
          {
            "in": "query",
            "name": "status",
            "schema": {
              "type": "string",
              "enum": [
                "approved",
                "pending",
                "draft",
                "closed"
              ]
            }
          },
          {
            "in": "query",
            "name": "search",
            "schema": {
              "type": "string"
            },
            "description": "Search by commitment number or title"
          },
          {
            "in": "query",
            "name": "companyId",
            "schema": {
              "type": "string"
            },
            "description": "Filter by company ID"
          }
        ],
        "responses": {
          "200": {
            "description": "Paginated list of commitments",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/Commitment"
                      }
                    },
                    "meta": {
                      "$ref": "#/components/schemas/PaginationMeta"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "Commitments"
        ],
        "summary": "Create a new commitment",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateCommitment"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Commitment created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Commitment"
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "422": {
            "$ref": "#/components/responses/ValidationError"
          }
        }
      }
    },
    "/api/commitments/{id}": {
      "delete": {
        "tags": [
          "Commitments"
        ],
        "summary": "Delete a commitment",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Commitment deleted successfully"
          },
          "404": {
            "description": "Commitment not found"
          }
        }
      }
    },
    "/api/chat": {
      "post": {
        "tags": [
          "RAG Chat"
        ],
        "summary": "Simple chat endpoint",
        "servers": [
          {
            "url": "http://localhost:8051"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "message"
                ],
                "properties": {
                  "message": {
                    "type": "string",
                    "description": "User's chat message"
                  },
                  "project_id": {
                    "type": "integer",
                    "description": "Optional project ID for context"
                  },
                  "limit": {
                    "type": "integer",
                    "default": 5,
                    "description": "Limit for retrieved documents"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Chat response with sources",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "reply": {
                      "type": "string"
                    },
                    "sources": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/Source"
                      }
                    },
                    "tasks": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/Task"
                      }
                    },
                    "insights": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/Insight"
                      }
                    }
                  }
                }
              }
            }
          },
          "422": {
            "$ref": "#/components/responses/ValidationError"
          }
        }
      }
    },
    "/api/rag-chat-simple": {
      "post": {
        "tags": [
          "RAG Chat"
        ],
        "summary": "Simple RAG chat (non-streaming)",
        "servers": [
          {
            "url": "http://localhost:8051"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "message"
                ],
                "properties": {
                  "message": {
                    "type": "string"
                  },
                  "history": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "role": {
                          "type": "string",
                          "enum": [
                            "user",
                            "assistant"
                          ]
                        },
                        "text": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RAG chat response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "response": {
                      "type": "string"
                    },
                    "retrieved": {
                      "type": "array",
                      "items": {
                        "type": "object"
                      }
                    }
                  }
                }
              }
            }
          },
          "422": {
            "$ref": "#/components/responses/ValidationError"
          },
          "503": {
            "description": "RAG workflow not available"
          }
        }
      }
    },
    "/rag-chatkit": {
      "post": {
        "tags": [
          "RAG Chat"
        ],
        "summary": "Main RAG ChatKit endpoint (streaming)",
        "servers": [
          {
            "url": "http://localhost:8051"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "description": "ChatKit protocol message"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Server-Sent Events stream",
            "content": {
              "text/event-stream": {
                "schema": {
                  "type": "string",
                  "description": "SSE stream of chat events"
                }
              }
            }
          },
          "503": {
            "description": "RAG workflow not available"
          }
        }
      }
    },
    "/api/rag-chatkit/bootstrap": {
      "get": {
        "tags": [
          "RAG Chat"
        ],
        "summary": "Bootstrap RAG chat session",
        "responses": {
          "200": {
            "description": "Initial chat state",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ChatState"
                }
              }
            }
          }
        }
      }
    },
    "/api/rag-chatkit/state": {
      "get": {
        "tags": [
          "RAG Chat"
        ],
        "summary": "Get RAG chat state",
        "parameters": [
          {
            "in": "query",
            "name": "thread_id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Current chat state",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ChatState"
                }
              }
            }
          }
        }
      }
    },
    "/api/auth/signup": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "summary": "Create new user account",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "email",
                  "password",
                  "fullName"
                ],
                "properties": {
                  "email": {
                    "type": "string",
                    "format": "email"
                  },
                  "password": {
                    "type": "string",
                    "minLength": 8
                  },
                  "fullName": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Account created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    },
                    "user": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid request or email already registered"
          }
        }
      }
    },
    "/api/ai/sql": {
      "post": {
        "tags": [
          "AI"
        ],
        "summary": "Generate SQL from natural language",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "prompt"
                ],
                "properties": {
                  "prompt": {
                    "type": "string",
                    "description": "Natural language query"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Generated SQL query",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "sql": {
                      "type": "string"
                    },
                    "explanation": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/docs-search": {
      "get": {
        "tags": [
          "Search"
        ],
        "summary": "Search documents",
        "parameters": [
          {
            "in": "query",
            "name": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "Search query"
          },
          {
            "in": "query",
            "name": "limit",
            "schema": {
              "type": "integer",
              "default": 10
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Search results",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "results": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/SearchResult"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/companies": {
      "get": {
        "tags": [
          "Companies"
        ],
        "summary": "List companies",
        "parameters": [
          {
            "in": "query",
            "name": "search",
            "schema": {
              "type": "string"
            },
            "description": "Search companies by name"
          }
        ],
        "responses": {
          "200": {
            "description": "List of companies",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Company"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/ingest/fireflies": {
      "post": {
        "tags": [
          "RAG Chat"
        ],
        "summary": "Ingest Fireflies meeting transcript",
        "servers": [
          {
            "url": "http://localhost:8051"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "path"
                ],
                "properties": {
                  "path": {
                    "type": "string",
                    "description": "Path to Fireflies transcript file"
                  },
                  "project_id": {
                    "type": "integer",
                    "description": "Associated project ID"
                  },
                  "dry_run": {
                    "type": "boolean",
                    "default": true,
                    "description": "Preview without saving"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Ingestion result",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "result": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Project": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "job_number": {
            "type": "string"
          },
          "client": {
            "type": "string"
          },
          "start_date": {
            "type": "string",
            "format": "date"
          },
          "end_date": {
            "type": "string",
            "format": "date"
          },
          "state": {
            "type": "string",
            "enum": [
              "pre-construction",
              "construction",
              "post-construction",
              "completed",
              "cancelled"
            ]
          },
          "phase": {
            "type": "string"
          },
          "estimated_revenue": {
            "type": "number"
          },
          "estimated_profit": {
            "type": "number"
          },
          "category": {
            "type": "string"
          },
          "is_flagged": {
            "type": "boolean"
          },
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "CreateProject": {
        "type": "object",
        "required": [
          "name",
          "client"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "job_number": {
            "type": "string"
          },
          "client": {
            "type": "string"
          },
          "start_date": {
            "type": "string",
            "format": "date"
          },
          "end_date": {
            "type": "string",
            "format": "date"
          },
          "state": {
            "type": "string"
          },
          "phase": {
            "type": "string"
          },
          "estimated_revenue": {
            "type": "number"
          },
          "estimated_profit": {
            "type": "number"
          },
          "category": {
            "type": "string"
          }
        }
      },
      "Commitment": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "number": {
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "status": {
            "type": "string",
            "enum": [
              "approved",
              "pending",
              "draft",
              "closed"
            ]
          },
          "type": {
            "type": "string",
            "enum": [
              "contract",
              "purchase_order"
            ]
          },
          "contract_company_id": {
            "type": "string"
          },
          "contract_company": {
            "$ref": "#/components/schemas/Company"
          },
          "assignee_id": {
            "type": "string"
          },
          "assignee": {
            "$ref": "#/components/schemas/User"
          },
          "description": {
            "type": "string"
          },
          "payment_terms": {
            "type": "string"
          },
          "contract_date": {
            "type": "string",
            "format": "date"
          },
          "start_date": {
            "type": "string",
            "format": "date"
          },
          "substantial_completion_date": {
            "type": "string",
            "format": "date"
          },
          "original_amount": {
            "type": "number"
          },
          "approved_change_orders": {
            "type": "number"
          },
          "revised_contract_amount": {
            "type": "number"
          },
          "billed_to_date": {
            "type": "number"
          },
          "balance_to_finish": {
            "type": "number"
          },
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time"
          }
        }
