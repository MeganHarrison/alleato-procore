{
  "name": "alleato-docs-viewer",
  "version": "1.0.0",
  "description": "Markdown documentation viewer for Alleato project",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "docs": "node server.js ../../",
    "dev": "nodemon server.js ../../",
    "install-global": "npm link",
    "uninstall-global": "npm unlink"
  },
  "bin": {
    "docs-viewer": "./server.js"
  },
  "keywords": ["markdown", "documentation", "viewer"],
  "author": "Alleato Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "marked": "^11.1.0",
    "highlight.js": "^11.9.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}