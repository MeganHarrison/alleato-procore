{
  "name": "alleato-procore",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "cd frontend && npx next dev",
    "dev:server": "cd backend && ./start.sh",
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:server\"",
    "build": "cd frontend && next build",
    "start": "cd frontend && next start",
    "lint": "cd frontend && eslint .",
    "update-types": "npx supabase gen types --lang=typescript --project-id lgveqfnpkxvzbnnwuled > frontend/database.types.ts",
    "test": "playwright test --config=frontend/playwright.config.ts",
    "test:ui": "playwright test --ui --config=frontend/playwright.config.ts",
    "test:headed": "playwright test --headed --config=frontend/playwright.config.ts",
    "test:screenshots": "playwright test frontend/tests/comprehensive-screenshots.spec.ts --config=frontend/playwright.config.ts",
    "test:auth": "playwright test frontend/tests/auth-verification.spec.ts --config=frontend/playwright.config.ts",
    "test:report": "playwright show-report",
    "test:visual": "playwright test --config=frontend/playwright.config.visual.ts",
    "test:visual:update": "playwright test --config=frontend/playwright.config.visual.ts --update-snapshots",
    "test:visual:report": "playwright show-report frontend/tests/visual-regression-report",
    "test:performance": "playwright test frontend/tests/performance-metrics.spec.ts --config=frontend/playwright.config.ts",
    "test:performance:report": "playwright show-report frontend/tests/performance-report",
    "test:a11y": "playwright test frontend/tests/accessibility.spec.ts --config=frontend/playwright.config.ts",
    "test:a11y:report": "playwright show-report frontend/tests/a11y-report",
    "seed:test": "node scripts/seed-test-data.js",
    "seed:test:full": "ts-node scripts/seed-test-data.ts",
    "seed:test:clear": "ts-node scripts/seed-test-data.ts --clean"
  },
